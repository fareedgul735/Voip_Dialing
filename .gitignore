        const ChevronDownIcon = () => (
            <svg className="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path strokeLinecap="round" strokeLinejoin="round" strokeWidth={2} d="M19 9l-7 7-7-7" />
            </svg>
        );

        const ChevronUpIcon = () => (
            <svg className="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path strokeLinecap="round" strokeLinejoin="round" strokeWidth={2} d="M5 15l7-7 7 7" />
            </svg>
        );

        const CloudPBXServices = () => {
            const [expandedService, setExpandedService] = useState(null);

            const services = [
                {
                    id: 1,
                    title: "Cloud PBX",
                    description: "Cloud PBX Everything your business needs",
                    icon: CloudIcon,
                    iconColor: "bg-orange-100 text-orange-600",
                    plans: [
                        "Enterprise",
                        "Enterprise",
                        "Enterprise",
                        "Enterprise",
                        "Enterprise",
                        "Enterprise",
                        "Enterprise",
                        "Enterprise"
                    ]
                },
                {
                    id: 2,
                    title: "Bulk SMS",
                    description: "Cloud PBX Everything your business needs",
                    icon: MessageIcon,
                    iconColor: "bg-orange-100 text-orange-600",
                    plans: [
                        "Enterprise",
                        "Enterprise",
                        "Enterprise",
                        "Enterprise",
                        "Enterprise",
                        "Enterprise",
                        "Enterprise",
                        "Enterprise"
                    ]
                },
                {
                    id: 3,
                    title: "VOIP Lines",
                    description: "Cloud PBX Everything your business needs",
                    icon: PhoneIcon,
                    iconColor: "bg-orange-100 text-orange-600",
                    plans: [
                        "Enterprise",
                        "Enterprise",
